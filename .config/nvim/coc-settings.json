{
    "languageserver": {
        "ccls": {
            "command": "ccls",
            "filetypes": [
                "c",
                "cpp",
                "objc",
                "objcpp"
            ],
            "rootPatterns": [
                ".ccls",
                "compile_commands.json",
                ".vim/",
                ".git/",
                ".hg/"
            ],
            "initializationOptions": {
                "cache": {
                    "directory": "/tmp/ccls"
                }
            }
        }
    },
    "snippets.extends": {
        "latex": ["tex", "texmath"]
    },
    "npm.binPath": "~/.nvm/versions/node/v17.5.0/bin/npm",
    "diagnostic.displayByAle": true,
    "coc.preferences.jumpCommand": "tabe"
}
